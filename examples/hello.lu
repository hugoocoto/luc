.include io;  // print
.include str; // copy, append

.start main;

let join = str ( str sl :: str sr ) {
        ret (#copy sl) #append sr;
}

let hello = none ( none :: none ) {
        stdout #print ("hello, " #join "World!");
}

main = hello;








